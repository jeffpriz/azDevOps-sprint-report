{
  "name": "SprintActivityReport",
  "version": "0.0.14",
  "description": "Sprint Backlog Activity Report",
  "keywords": [
    "extensions",
    "Azure DevOps",
    "Visual Studio Team Services"
  ],
  "license": "GPL-3.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/jeffpriz/azDevOps-sprint-report.git"
  },
  "scripts": {
    "clean": "rimraf ./dist",
    "compile": "npm run clean && webpack --mode production",
    "compile:dev": "npm run clean && webpack --mode development",
    "build": "npm run compile",
    "build:dev": "npm run compile:dev && npm run postbuild",
    "postbuild": "npm run package-extension -- --rev-version",
    "package-extension": "tfx extension create --manifest-globs azure-devops-extension.json src/ReportPanel/Panel/Panel.json",
    "publish-extension": "tfx extension publish --manifest-globs azure-devops-extension.json src/ReportPanel/Panel/Panel.json"
  },
  "dependencies": {
    "azure-devops-extension-api": "~1.157.0",
    "azure-devops-extension-sdk": "~2.0.11",
    "azure-devops-ui": "~2.166.3",
    "react": "~16.8.1",
    "react-dom": "~16.8.1"
  },
  "devDependencies": {
    "@types/react": "~16.8.2",
    "@types/react-dom": "~16.8.0",
    "base64-inline-loader": "^2.0.1",
    "copy-webpack-plugin": "^11.0.0",
    "css-loader": "~6.7.3",
    "file-loader": "~6.2.0",
    "node-sass": "~4.13.1",
    "rimraf": "~2.6.2",
    "sass-loader": "~13.2.0",
    "style-loader": "~3.3.1",
    "tfx-cli": "^0.6.3",
    "ts-loader": "~9.4.2",
    "typescript": "^2.9.2",
    "webpack": "^5.75.0",
    "webpack-cli": "^5.0.1"
  }
}
