{
    "manifestVersion": 1,
    "id": "sprintActivityReport",
    "version": "0.0.7",
    "name": "Sprint Backlog Activity Report",
    "scopes": ["vso.work", "vso.extension.data_write"],
    "description": "Report on backlog item movement within a sprint.  Gives you insight in to things like How many stories to we push out of this sprint? or How many stories did we add to this sprint?.",
    "publisher": "OneLuckiDev",
    "public": false,
    "icons": {
      "default": "1ld/img/logo.png"
    },
    "targets": [
      {
        "id": "Microsoft.VisualStudio.Services"
      }
    ],
    "categories": ["Azure Boards"],
    "tags": ["sprint", "iteration", "report", "moved story"],
    "content": {
      "details": {
        "path": "details.md"
      },
      "license": {
        "path": "LICENSE"
      }
    },
    "links": {
      "support": {
        "uri": "https://github.com/jeffpriz/azDevOps-sprint-report/issues"
      },
      "repository": {
        "uri": "https://github.com/jeffpriz/azDevOps-sprint-report"
      }
    },
    "repository": {
      "type": "git",
      "uri": "https://github.com/jeffpriz/azDevOps-sprint-report"
    },
    "branding": {
      "color": "rgb(27,160,225)",
      "theme": "dark"
    },
    "files": [
      {
        "path": "dist",
        "addressable": true
      },
      {
        "path": "toolpane.html",
        "addressable": true
      },
      {
        "path": "node_modules/vss-web-extension-sdk/lib",
        "addressable": true,
        "packagePath": "lib"
      }
    ],
    "contributions": [
      {
        "id": "sprintBacklogReport",
        "type": "ms.vss-work-web.backlog-panel",        
        "description": "Report on Sprint backlog activity.",
        "targets": ["ms.vss-work-web.iteration-backlog-toolpane"],
        "properties": {
          "uri": "toolpane.html",
          "name": "Sprint Backlog Activity",
          "title": "Sprint Backlog Activity Report",
          "registeredObjectId": "sprintBacklogReport"
        }
      }
    ]
  }
  